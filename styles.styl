#toggle 
  position: relative
  width: 36px
  height: 36px
  margin: -19px auto
  left: 50%
  @media only screen and (max-width: 835px)
    left: 46%;
  input 
    position: absolute
    top: -3px
    left: -3px
    width: 100%
    height: 100%
    z-index: 1
    opacity: 0
    cursor: pointer
    &:checked 
      +span 
        background: transparent
        &:after 
          transform: rotate(45deg) translate(-1px, -12px)
        
        &:before 
          transform: rotate(-45deg) translate(-4px, 16px)
        

  
  span 
    position: absolute
    width: 100%
    height: 6px
    background: #4c4e51
    margin-top: 10px
    transition: all 0.1s
    border-radius: 2px;
    &:before
    &:after 
      border-radius: 2px;
      content: ""
      position: absolute
      width: 100%
      height: 2px
      background: #4c4e51
      top: -12px
      transition: all 0.3s
    
    &:after 
      top: 12px
      height 6px
    &:before    
        height 6px
    
