        input[type="checkbox"]
            margin-right: 60px;
            margin-top: 16px;
            position: relative;
            display: inline-block;
            -webkit-appearance: none;
            height: 1em;
            width: 2.5em;
            font-size: 2em;
            border-radius: 1.5em;
            background-color: #eaeaea;
            border-color: transparent;
            background-clip: padding-box;
            color: #BFBEBF;
            vertical-align: middle;
            transition: all 0.25s linear 0.25s;
            cursor: pointer;
            outline: none;
            @media only screen and (max-width: 1050px)
                margin-right: 5px;
            &:checked
                &:before
                    animation: switchbox-on-transform 0.25s ease-out forwards;
                    @-webkit-keyframes switchbox-on-transform {
                        0%   {left:0px; top:-5px; transform:scale(1); background-color: #fff}
                        25%  {top:-5px; transform:scale(1.2, 0.6)}
                        50%  {top:-5px; transform:scale(1.2, 0.6)}
                        75%  {top:-5px; transform:scale(1.2, 0.6)}
                        100% {left:35px; top:-5px; transform:scale(1); background-color: rgba(4,255,0,0.38)}
                    }
            animation: switchbox-off-transform 0.25s ease-out forwards;
            @-webkit-keyframes switchbox-off-transform {
                0%   {left:35px; top:-5px; transform:scale(1);}
                25%  {top:-5px; transform:scale(1.2, 0.6)}
                50%  {top:-5px; transform:scale(1.2, 0.6)}
                75%  {top:-5px; transform:scale(1.2, 0.6)}
                100% {left:0px; top:-5px; transform:scale(1)}
            }
            &::before
                content: " ";
                position: absolute;
                height: 1.2em;
                width: 1.2em;
                top: -5px;
                left: -1px;
                bottom: 0;
                right: 50%;
                background-color: #FFFFFF;
                border-radius: 100%;
                background-clip: padding-box;
                z-index: 2;
                transform-origin: right center;
                box-shadow: 0 0 1px 1px rgba(50, 50, 50, 0.3);
                animation: switchbox-off-transform 0.25s ease-out forwards;
                transition: background-color 0.25s linear 0.25s;

                
